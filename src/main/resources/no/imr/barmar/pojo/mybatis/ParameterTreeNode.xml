<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper      
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"      
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="no.imr.barmar.pojo.ParameterTreeNodeMapper"> 

	<resultMap id="ParameterTreeResult" type="no.imr.barmar.pojo.mybatis.ParameterTreeNode">
    	<id property="id" column="id"/>
    	<result property="idParametertree" column="id_parametertree"/>
    	<result property="idParameter" column="id_parameter"/>
    	<result property="text" column="name"/>
	</resultMap>

	<sql id="parametertreeSelect">
   		SELECT id, id_parametertree, id_parameter, name
    </sql>
    
	<select id="getAllTreeNodes" resultMap="ParameterTreeResult" resultType="java.util.ArrayList">
    	<include refid="parametertreeSelect" />
   		FROM parametertree
	</select>
				
</mapper>